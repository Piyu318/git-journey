class Volume {

    void calculateVolume(double side) {
        System.out.println("Cube Volume: " + (side * side * side));
    }

    void calculateVolume(double l, double w, double h) {
        System.out.println("Rectangular Cube Volume: " + (l * w * h));
    }

    void calculateVolume(double radius, boolean sphere) {
        System.out.println("Sphere Volume: " + (4.0 / 3 * Math.PI * radius * radius * radius));
    }

    public static void main(String[] args) {
        Volume v = new Volume();
        v.calculateVolume(3);
        v.calculateVolume(4, 5, 6);
        v.calculateVolume(7, true);
    }
}
