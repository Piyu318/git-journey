import java.util.Scanner;

public class DigitIncrement {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Enter a five-digit number: ");
        String input = scanner.nextLine();
        
        // Validate input: exactly 5 digits, all numeric
        if (input.length() != 5 || !input.matches("\\d{5}")) {
            System.out.println("Invalid input. Please enter exactly five digits (e.g., 12345).");
            return;
        }
        
        // Extract digits into an array
        int[] digits = new int[5];
        for (int i = 0; i < 5; i++) {
            digits[i] = input.charAt(i) - '0';
        }
        
        // Process each digit from right to left, adding 1 and handling carry
        int carry = 0;
        int[] newDigits = new int[6]; // Array to hold up to 6 digits in case of carry
        int index = 5; // Start filling from the end
        
        for (int i = 4; i >= 0; i--) {
            int sum = digits[i] + 1 + carry;
            newDigits[index--] = sum % 10;
            carry = sum / 10;
        }
        
        // If there's a remaining carry, add it as the highest digit
        if (carry > 0) {
            newDigits[index--] = carry;
        }
        
        // Build the result string, skipping unused leading positions
        StringBuilder result = new StringBuilder();
        for (int i = index + 1; i <= 5; i++) {
            result.append(newDigits[i]);
        }
        
        System.out.println("New number: " + result.toString());
        
        scanner.close();
    }
}
